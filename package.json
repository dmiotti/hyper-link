{
  "name": "@muxumuxu/hyper-link",
  "version": "0.2.3",
  "main": "./dist/hyper-link.umd.js",
  "unpkg": "dist/hyper-link.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name hyper-link ./src/components/index.js",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "jest": "^23.6.0",
    "vue-jest": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/muxumuxu/hyper-link.git"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "keywords": [
    "nuxt",
    "nuxtjs",
    "link",
    "hypertext",
    "vue",
    "vuejs",
    "muxumuxu",
    "js",
    "javascript",
    "helper",
    "tool"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "license": "MIT"
}
